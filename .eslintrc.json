{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": "off",
        "no-console": "warn",
        "max-len": ["error", { "code": 150 }],
        "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
        "no-param-reassign": ["error", { "props": false }],
        "comma-dangle": ["error","always-multiline"],
        "no-underscore-dangle": 0,
        "linebreak-style": ["error", "windows"]
    }
}
